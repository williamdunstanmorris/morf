pipeline {
    agent { docker { image 'golang' } }
    stages {
        stage('build') {
            steps {
                sh 'go version'
            }
        }
        stage('test') {
            steps {
                sh 'go version'
            }
        }
        stage('package') {
            steps {
                sh 'go version'
            }
        }
        stage('install') {
            steps {
                sh 'go version'
            }
        }
    }
}
